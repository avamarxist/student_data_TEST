<html>
    <head>
        <%- include('../partials/head') %>
    </head>

    <body>
        <h1> Current staff </h1>
        <% for(let staff of data) { %>
            <article>
                <h2><%= `${staff.lName}, ${staff.fName}` %></h2>
                <p><%= `Status: ${staff.status}` %></p>
                <% for(let course of staff.courses) { %>
                    <p><%= `${course.name}` %></p>
                    <p><%= `${course.name}` %></p>
                <% } %>
            </article>
        <% } %>
    </body>
</html>